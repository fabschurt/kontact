{
  "name": "fabschurt/kontact",
  "description": "Contact form backend for static websites.",
  "type": "project",
  "homepage": "https://github.com/fabschurt/kontact",
  "keywords": ["contact", "backend", "static", "website"],
  "license": "MIT",
  "authors": [
    {
      "name": "Fabien Schurter",
      "email": "fabien@fabschurt.com",
      "homepage": "http://fabschurt.com/",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "dev@fabschurt.com",
    "issues": "https://github.com/fabschurt/kontact/issues",
    "source": "https://github.com/fabschurt/kontact/tree/master",
    "docs": "https://github.com/fabschurt/kontact/blob/master/README.md"
  },
  "require": {
    "php": "^7.0",
    "vlucas/phpdotenv": "^2.4",
    "pimple/pimple": "^3.0",
    "silex/silex": "^2.0",
    "symfony/form": "^2.8|^3.0",
    "symfony/http-foundation": "^2.8|^3.0",
    "symfony/options-resolver": "^2.8|^3.0",
    "symfony/validator": "^2.8|^3.0",
    "swiftmailer/swiftmailer": "^5.4",
    "fabschurt/silex-provider-captcha": "^1.0@dev",
    "junker/symfony-jsend-response": "^0.1"
  },
  "require-dev": {
    "consolidation/robo": "^1.0",
    "friendsofphp/php-cs-fixer": "^1.12",
    "phpunit/phpunit": "^5.6",
    "phpunit/php-invoker": "^1.1",
    "codeception/specify": "^0.4",
    "codeception/verify": "^0.3",
    "symfony/browser-kit": "^2.8|^3.0"
  },
  "autoload": {
    "psr-4": {
      "FabSchurt\\Kontact\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FabSchurt\\Kontact\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
